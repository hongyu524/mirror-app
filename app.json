{
  "expo": {
    "name": "Mirror App",
    "slug": "mirror-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mirror_logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/mirror_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hesra.mirror",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mirror_logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/mirror_logo.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Mirror to access your photos to attach them to moments."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "2eb072e2-c900-4dde-898f-50ff9595f71c"
      }
    }
  }
}
